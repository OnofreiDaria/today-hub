{
  "name": "new-project",
  "version": "1.0.0",
  "description": "New project",
  "main": "index.js",
  "dependencies": {
    "cssnano": "^4.1.7",
    "gulp-file-include": "^2.0.1",
    "gulp-include": "^2.3.1"
  },
  "config": {
    "src": {
      "sass": "src/sass/main.scss",
      "html": "src/*.html",
      "js": "src/js/main.js",
      "fonts": "src/fonts/**/*.*",
      "img": {
        "all": [
          "src/img/**/*.*",
          "!src/img/noCompress/**/*.*"
        ],
        "noCompress": "src/img/noCompress/**/*.*"
      }
    },
    "dist": {
      "css": "dist/css",
      "html": "dist",
      "js": "dist/js/",
      "fonts": "dist/fonts/",
      "img": "dist/img"
    },
    "libs": {
      "js": "src/js/libs.js",
      "css": "src/css/libs.css"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-imagemin": "^5.0.3",
    "gulp-import-css": "^0.1.3",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^3.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "jquery": "^3.3.1",
    "node-sass-tilde-importer": "^1.0.2",
    "pump": "^3.0.0",
    "require-dir": "^1.1.0",
    "rfs": "^7.1.8",
    "run-sequence": "^2.2.1"
  },
  "scripts": {
    "start": "gulp"
  },
  "author": "Daria Onofrei",
  "license": "ISC"
}
